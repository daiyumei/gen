<template>
    <name>checkbox</name>
    <filePath>src/main/java/com/meizu/genbatis/tmp</filePath>
    <fileName>${ClassName}.checkbox</fileName>
    <content>
        <![CDATA[
            <div id="${type}-div">
                <#list checkboxList as l>
                <input type="${type}" name="${l.id}" class="${type}" value="${l.value}" id="${l.id}" <#if l_index==0>checked="checked"</#if>/><lable for="${l.id}">${l.content}</lable><br/>
                </#list>
            </div>
        ]]>
    </content>
</template>